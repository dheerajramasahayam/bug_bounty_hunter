{
  "name": "bughunter-ai",
  "version": "1.0.0",
  "description": "AI-powered bug bounty automation tool with Gemini 3.0 Pro integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cp -r src/dashboard/public dist/dashboard/",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "cli": "tsx src/cli/index.ts",
    "dashboard": "tsx src/dashboard/server.ts",
    "test": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bug-bounty",
    "security",
    "vulnerability-scanner",
    "ai",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.7.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "crawlee": "^3.12.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "googleapis": "^169.0.0",
    "got": "^14.4.5",
    "ora": "^8.1.1",
    "puppeteer": "^23.11.1",
    "robots-parser": "^3.0.1",
    "uuid": "^11.0.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
